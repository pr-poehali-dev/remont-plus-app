      "method": "GET",
      "path": "/",
      "expectedStatus": 400,
      "expectedBody": {
        "error": "project_id is required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Upload photo without data",
      "method": "POST",
      "path": "/",
      "body": {
